# YouTrack API Configuration
YOUTRACK_BASE_URL=https://realbrokerage.youtrack.cloud
YOUTRACK_TOKEN=your_youtrack_token_here

# Google OAuth Configuration
GOOGLE_CLIENT_ID=your_google_client_id_here
GOOGLE_CLIENT_SECRET=your_google_client_secret_here

# JWT Secret (generate with: openssl rand -base64 32)
JWT_SECRET=your_jwt_secret_here

# PostgreSQL Configuration (for metrics)
POSTGRES_HOST=your_postgres_host
POSTGRES_PORT=5432
POSTGRES_DATABASE=your_database_name
POSTGRES_USER=your_postgres_user
POSTGRES_PASSWORD=your_postgres_password

# Metabase Configuration (for card inspector tool)
METABASE_BASE_URL=https://metabase.therealbrokerage.com
METABASE_API_KEY=your_metabase_api_key_here
