{
  "name": "youtrack-frontend",
  "version": "1.0.0",
  "description": "Modern ticketing and reporting UI for YouTrack",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run proxy\" \"vite\" --names \"proxy,vite\" --prefix-colors \"blue,green\"",
    "vite-only": "vite",
    "proxy": "node server/proxy.js",
    "start": "node server/production-server.js",
    "build": "tsc --noEmit && vite build",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist",
    "build:render": "npm run build && npm run start"
  },
  "dependencies": {
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "clsx": "^2.0.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^17.3.1",
    "express": "^4.18.2",
    "google-auth-library": "^10.4.1",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.294.0",
    "node-fetch": "^3.3.2",
    "pg": "^8.16.3",
    "postcss": "^8.4.32",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^6.27.0",
    "recharts": "^2.8.0",
    "tailwindcss": "^3.3.6",
    "typescript": "^5.3.0"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.9",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.10.0",
    "@vitejs/plugin-react": "^4.3.0",
    "autoprefixer": "^10.4.16",
    "concurrently": "^9.2.1",
    "vite": "^5.4.0"
  },
  "keywords": [
    "vite",
    "react",
    "typescript",
    "youtrack",
    "ticketing",
    "reporting"
  ],
  "overrides": {
    "punycode": "^2.3.1"
  },
  "license": "MIT"
}
