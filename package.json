{
  "name": "youtrack-frontend",
  "version": "1.0.0",
  "description": "Modern ticketing and reporting UI for YouTrack",
  "main": "index.js",
  "scripts": {
    "develop": "gatsby develop",
    "dev": "concurrently \"npm run proxy\" \"npm run gatsby-only\" --names \"proxy,gatsby\" --prefix-colors \"blue,green\"",
    "proxy": "node server/proxy.js",
    "gatsby-only": "gatsby develop",
    "start": "node server/production-server.js",
    "build": "NODE_NO_WARNINGS=1 gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "type-check": "tsc --noEmit",
    "deploy": "gatsby build && gh-pages -d public",
    "build:render": "npm run build && npm run start"
  },
  "dependencies": {
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "clsx": "^2.0.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "gatsby": "^5.13.0",
    "gatsby-plugin-image": "^3.13.0",
    "gatsby-plugin-postcss": "^6.13.0",
    "gatsby-plugin-sharp": "^5.13.0",
    "gatsby-source-filesystem": "^5.13.0",
    "gatsby-transformer-sharp": "^5.13.0",
    "google-auth-library": "^10.4.1",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.294.0",
    "node-fetch": "^3.3.2",
    "pg": "^8.16.3",
    "postcss": "^8.4.32",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^10.1.0",
    "recharts": "^2.8.0",
    "tailwindcss": "^3.3.6",
    "typescript": "^5.3.0"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.9",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.10.0",
    "autoprefixer": "^10.4.16",
    "concurrently": "^9.2.1",
    "gh-pages": "^6.1.0"
  },
  "keywords": [
    "gatsby",
    "react",
    "typescript",
    "youtrack",
    "ticketing",
    "reporting"
  ],
  "overrides": {
    "punycode": "^2.3.1"
  },
  "license": "MIT"
}
